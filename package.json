    "@rollup/plugin-node-resolve": "^8.0.1",
    "@types/dedent": "^0.7.0",
    "@types/highlight.js": "^9.12.4",
    "@types/jest": "^24.9.0",
    "@types/katex": "^0.11.0",
    "@types/marked": "^0.7.4",
    "@types/node": "^10.13.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "ansi_up": "^4.0.4",
    "arrify": "^2.0.1",
    "common-path-prefix": "^3.0.0",
    "core-js": "^3.6.4",
