    "test": "jest --detectOpenHandles --coverage --verbose",
    "version": "./scripts/bump-version && git add README.adoc **/package.json",
    "watch-ts": "ttsc --build --watch"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
