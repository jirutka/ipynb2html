{
  "name": "ipynb2html-parent",
  "version": "0.4.0-rc.1",
  "private": true,
  "scripts": {
    "build": "ttsc --build",
    "bundle": "wsrun --exclude-missing bundle",
    "clean": "rimraf coverage/ dist/ lib/ .eslintcache *.log && wsrun clean",
    "lint": "eslint --cache --ext .ts,.tsx,.js .",
    "postinstall": "patch-package && run-s build",
