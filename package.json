    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "fs-extra": "^8.1.0",
    "jest": "^25.1.0",
    "jest-chain": "^1.1.2",
    "node-html-parser": "^1.2.19",
    "nodom": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.2.0",
